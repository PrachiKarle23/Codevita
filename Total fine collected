#include <iostream>
using namespace std;

int main() {
   
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++){
       cin>>a[i];
   }
   int X,D;
   cin>>X>>D;
    int cnt1=0,cnt2=0;
    for(int i=0;i<n;i++){
           if(a[i]%2==0){
               cnt1++;
           }
           else{
               cnt2++;
           }
    }
   if(D%2!=0){
        cout<<X*cnt1;
   }
   else{
       cout<<X*cnt2;
   }
  
   return 0;
}
