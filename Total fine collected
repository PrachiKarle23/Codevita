#include <iostream>
using namespace std;

int main() {
   
   int n;
   cin>>n;
   int a[n];
   for(int i=0;i<n;i++){
       cin>>a[i];
   }
   int X,D;
   cin>>X>>D;
   int cnt=0;
   if(D%2!=0){
       for(int i=0;i<n;i++){
           if(a[i]%2==0){
               cnt++;
           }
       }
   }
   else{
       for(int i=0;i<n;i++){
           if(a[i]%2!=0){
               cnt++;
           }
       }
   }
   cout<<X*cnt;
   return 0;
}
